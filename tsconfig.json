{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "declaration": false,
    "pretty": true,
    "sourceMap": true,
    "inlineSources": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "allowJs": true,
    "alwaysStrict": true,
    "removeComments": false,
    "preserveConstEnums": true
  },
  "exclude": [
    ".js", ".dev", ".prod", ".uat", ".staging"
  ]
}